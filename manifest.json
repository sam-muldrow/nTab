{
    "manifest_version": 2,
    "name": "algoDash",
    "version": "0.1",
    "description": "Your productivity dashboard",
    "author": "Sam Muldrow - Algo Labs",
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "permissions": [
      "storage",
      "topSites",
      "bookmarks"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }