{
    "manifest_version": 3,
    "name": "nTab",
    "version": "0.1",
    "description": "Your productivity dashboard",
    "author": "Sam Muldrow - Seacoast Software",
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "content_security_policy": {
      "default-src": "https://www.googleapis.com/*",
      "script-src": ["./thirdParty/api.js","./thirdParty/client"]
    },
    "host_permissions": [
      "http://suggestqueries.google.com/*"
  ],
    "oauth2": {
      "client_id": "948598366001-hc995g17s2fe4ephgr9m6684n03qjsme.apps.googleusercontent.com",
      "scopes":["https://www.googleapis.com/auth/userinfo.email", "https://www.googleapis.com/auth/calendar.readonly"]
    },
    "permissions": [
      "identity",
      "storage"
    ],
    "key": ""
  }